    "retry_scenarios": {
      "description": "重试场景的预期结果",
      "test_cases": [
        {
          "source_message": {
            "id": 1007,
            "type": "text",
            "content": "测试消息"
          },
          "target_channels": ["target1"],
          "initial_error": "FloodWait",
          "retry_delay": 1,
          "expected_result": {
            "success": true,
            "forwarded_count": 1,
            "failed_count": 0,
            "message_ids": [2009],
            "errors": [],
            "retry_count": 1,
            "total_duration": 1.5
          }
        },
        {
          "source_message": {
            "id": 1008,
            "type": "photo"
          },
          "target_channels": ["target1"],
          "initial_error": "NetworkError",
          "max_retries": 3,
          "expected_result": {
            "success": false,
            "forwarded_count": 0,
            "failed_count": 1,
            "message_ids": [],
            "errors": [
              {
                "channel": "target1",
                "error": "NetworkError",
                "message": "网络错误，重试3次后失败"
              }
            ],
            "retry_count": 3
          }
        }
      ]
    },
