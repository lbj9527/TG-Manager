{
    "keyword_filtering": {
      "description": "关键词过滤结果",
      "test_cases": [
        {
          "message": "这是包含重要关键词的消息",
          "keywords": ["重要", "紧急"],
          "should_pass": true,
          "reason": "包含关键词'重要'"
        },
        {
          "message": "这是普通的日常消息",
          "keywords": ["重要", "紧急"],
          "should_pass": false,
          "reason": "不包含任何关键词"
        },
        {
          "message": "紧急通知：系统维护",
          "keywords": ["重要", "紧急", "维护"],
          "should_pass": true,
          "reason": "包含关键词'紧急'和'维护'"
        },
        {
          "message": "URGENT: System Update",
          "keywords": ["urgent", "important"],
          "should_pass": true,
          "reason": "包含关键词'urgent'（不区分大小写）"
        }
      ]
    },
    "media_type_filtering": {
      "description": "媒体类型过滤结果",
      "test_cases": [
        {
          "message_type": "photo",
          "allowed_types": ["photo", "video"],
          "should_pass": true,
          "reason": "图片类型在允许列表中"
        },
        {
          "message_type": "sticker",
          "allowed_types": ["photo", "video"],
          "should_pass": false,
          "reason": "贴纸类型不在允许列表中"
        },
        {
          "message_type": "document",
          "allowed_types": ["photo", "video", "document"],
          "should_pass": true,
          "reason": "文档类型在允许列表中"
        },
        {
          "message_type": "voice",
          "allowed_types": ["audio", "voice"],
          "should_pass": true,
          "reason": "语音类型在允许列表中"
        }
      ]
    },
    "forward_filtering": {
      "description": "转发消息过滤结果",
      "test_cases": [
        {
          "is_forwarded": true,
          "exclude_forwards": true,
          "should_pass": false,
          "reason": "转发消息被排除"
        },
        {
          "is_forwarded": true,
          "exclude_forwards": false,
          "should_pass": true,
          "reason": "允许转发消息"
        },
        {
          "is_forwarded": false,
          "exclude_forwards": true,
          "should_pass": true,
          "reason": "原创消息通过"
        }
      ]
    },
    "reply_filtering": {
      "description": "回复消息过滤结果",
      "test_cases": [
        {
          "is_reply": true,
          "exclude_replies": true,
          "should_pass": false,
          "reason": "回复消息被排除"
        },
        {
          "is_reply": true,
          "exclude_replies": false,
          "should_pass": true,
          "reason": "允许回复消息"
        },
        {
          "is_reply": false,
          "exclude_replies": true,
          "should_pass": true,
          "reason": "非回复消息通过"
        }
      ]
    },
    "text_filtering": {
      "description": "纯文本消息过滤结果",
      "test_cases": [
        {
          "has_media": false,
          "exclude_text": true,
          "should_pass": false,
          "reason": "纯文本消息被排除"
        },
        {
          "has_media": false,
          "exclude_text": false,
          "should_pass": true,
          "reason": "允许纯文本消息"
        },
        {
          "has_media": true,
          "exclude_text": true,
          "should_pass": true,
          "reason": "媒体消息不受文本排除影响"
        }
      ]
    },
    "link_filtering": {
      "description": "链接过滤结果",
      "test_cases": [
        {
          "has_links": true,
          "exclude_links": true,
          "should_pass": false,
          "reason": "包含链接的消息被排除"
        },
        {
          "has_links": true,
          "exclude_links": false,
          "should_pass": true,
          "reason": "允许包含链接的消息"
        },
        {
          "has_links": false,
          "exclude_links": true,
          "should_pass": true,
          "reason": "不包含链接的消息通过"
        }
      ]
    },
    "combined_filtering": {
      "description": "组合过滤条件结果",
      "test_cases": [
        {
          "message_props": {
            "text": "重要通知：系统更新",
            "has_media": false,
            "is_forwarded": false,
            "is_reply": false,
            "has_links": false
          },
          "filter_config": {
            "keywords": ["重要"],
            "exclude_forwards": true,
            "exclude_replies": true,
            "exclude_text": false,
            "exclude_links": true
          },
          "should_pass": true,
          "reason": "满足所有过滤条件"
        },
        {
          "message_props": {
            "text": "普通消息",
            "has_media": false,
            "is_forwarded": true,
            "is_reply": false,
            "has_links": false
          },
          "filter_config": {
            "keywords": ["重要"],
            "exclude_forwards": true,
            "exclude_replies": true,
            "exclude_text": false,
            "exclude_links": true
          },
          "should_pass": false,
          "reason": "转发消息被排除且不包含关键词"
        },
        {
          "message_props": {
            "text": "查看链接 https://example.com",
            "has_media": false,
            "is_forwarded": false,
            "is_reply": false,
            "has_links": true
          },
          "filter_config": {
            "keywords": [],
            "exclude_forwards": false,
            "exclude_replies": false,
            "exclude_text": false,
            "exclude_links": true
          },
          "should_pass": false,
          "reason": "包含链接被排除"
        }
      ]
    },
    "edge_cases": {
      "description": "边界情况过滤结果",
      "test_cases": [
        {
          "message_props": {
            "text": "",
            "has_media": true,
            "media_type": "photo"
          },
          "filter_config": {
            "keywords": ["重要"],
            "media_types": ["photo"]
          },
          "should_pass": false,
          "reason": "空文本不包含关键词"
        },
        {
          "message_props": {
            "text": "重要",
            "has_media": true,
            "media_type": "sticker"
          },
          "filter_config": {
            "keywords": ["重要"],
            "media_types": ["photo"]
          },
          "should_pass": false,
          "reason": "贴纸类型不在允许列表中"
        },
        {
          "message_props": {
            "text": "IMPORTANT",
            "has_media": false
          },
          "filter_config": {
            "keywords": ["important"],
            "exclude_text": false
          },
          "should_pass": true,
          "reason": "关键词匹配（不区分大小写）"
        }
      ]
    }
  }